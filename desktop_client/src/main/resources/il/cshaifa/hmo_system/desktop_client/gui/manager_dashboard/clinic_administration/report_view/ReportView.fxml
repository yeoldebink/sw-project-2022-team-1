<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefWidth="300.0" xmlns="http://javafx.com/javafx/14" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="il.cshaifa.hmo_system.desktop_client.gui.manager_dashboard.clinic_administration.report_view.ReportViewController"
  stylesheets="@../../../hmo_lite.css">
    <children>
        <VBox prefWidth="270.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <children>
                <Label fx:id="report_type_label" text="Placeholder Text">
                    <font>
                        <Font size="26.0"/>
                    </font>
                </Label>
                <TableView fx:id="report_table" prefWidth="280.0" VBox.vgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="keyColumn" prefWidth="75.0" text="Key"/>
                        <TableColumn fx:id="valueColumn" prefWidth="75.0" text="Value"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>
